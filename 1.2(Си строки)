#include <iostream>
#include <cstring>

using namespace std;

int main()
{ 
 char memory, memCheck;
 int stek, stek1;
 bool flag = false, flag1 = false;
 int maxcounter = 0;
 char s[100], counterEnd[100], counterStart[100];
 cin.getline(s, 100);
 char *str = strtok(s, " ");
 char bufer[100][100];
 while(str != NULL)
 {
 for(int i = 0; i < strlen(str); i++)
 {
 bufer[maxcounter][i] = str[i]; // буфер слов
 counterEnd[maxcounter] = str[(strlen(str) - 1)]; // буфер конечных букв слов
 counterStart[maxcounter] = str[0]; // буфер начальных букв слов
 }
 maxcounter++;
 str = strtok(NULL, " "); 
 }
 for(int i = 0; i < maxcounter; i++)
 {
 for(int j = 0; j < maxcounter; j++)
 {
 if(counterEnd[i] == counterStart[j]) 
 { 
 cout << bufer[i] << " " << bufer[j] << " ";
 flag = true;
 stek = i;
 stek1 = j;
 memory = counterEnd[j];
 memCheck = memory;

 }
 }
 if(flag == true)
 break;
 } 
 for(int i = stek; i < maxcounter - 1; i++)
 { 
 if(strlen(bufer[i]) >= strlen(bufer[i+1]))
 {
 for(int j = 0; j < strlen(bufer[i]); j++)
 {
 bufer[i][j] = bufer[i+1][j];
 }
 }
 else
 {
 for(int j = 0; j < strlen(bufer[i+1]); j++)
 {
 bufer[i][j] = bufer[i+1][j];
 }
 }
 }// первое слово
 for(int i = (stek1-1); i < maxcounter - 2; i++)
 { 
 if(strlen(bufer[i]) >= strlen(bufer[i+1]))
 {
 for(int j = 0; j < strlen(bufer[i]); j++)
 {
 bufer[i][j] = bufer[i+1][j];
 }
 }
 else
 {
 for(int j = 0; j < strlen(bufer[i+1]); j++)
 {
 bufer[i][j] = bufer[i+1][j];
 }
 }
 }// второе слово
 for(int i = stek ; i < maxcounter -1; i++)
 {
 counterStart[i] = counterStart[i+1];
 counterEnd[i] = counterEnd[i+1];
 }// нач и конеч буквы первого слова
 for(int i = (stek1-1); i < maxcounter - 2; i++)
 {
 counterStart[i] = counterStart[i+1];
 counterEnd[i] = counterEnd[i+1]; 
 }// нач и конеч буквы второго слова
 maxcounter = maxcounter - 2;
 while(maxcounter != 0)
 {
 for(int i = 0; i < maxcounter; i++)
 {
 if(memory == counterStart[i])
 {
 cout << bufer[i] << " ";
 memory = counterEnd[i];
 flag1 = true; 
 stek = i; 
 }
 if(flag1 == true)
 break;
 }
 if(memory == memCheck)
 return 0;
 memCheck = memory;
 flag1 = false;
 --maxcounter;
 for(int i = stek; i < maxcounter; i++)
 { 
 if(strlen(bufer[i]) >= strlen(bufer[i+1]))
 {
 for(int j = 0; j < strlen(bufer[i]); j++)
 {
 bufer[i][j] = bufer[i+1][j];
 }
 }
 else
 {
 for(int j = 0; j < strlen(bufer[i+1]); j++)
 {
 bufer[i][j] = bufer[i+1][j];
 }
 }
 }// снова зануление слова
 for(int i = stek ; i < maxcounter; i++)
 {
 counterStart[i] = counterStart[i+1];
 counterEnd[i] = counterEnd[i+1];
 }// зануление букв
 } 
}
